#include 
using namespace std;

double Root(double A)
{
        double a = 1.0;
        

        for(int i=0; i<100; i++)
        {
                a = 0.5*(a + A / a);
        }
        return a;
}


void main()
{
         int input;
         cout << "제곱근을 구할 숫자를 입력해주세요." << endl;
         cin >> input;
         Root(input);
         cout << "루트 " << input << " 은(는) " << Root(input) << "입니다." << endl;
}
