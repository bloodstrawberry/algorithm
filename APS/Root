double Root(double A)
{
        double a = 1.0;
        

        for(int i=0; i<100; i++)
        {
                a = 0.5*(a + A / a);
        }
        return a;
}

void root(double r)
{
	int i;

	for (i = 0; i < 100; i++)
	{

		r = (r + 2 / r) / 2;

		printf("%.10lf\n", r);
	}
}

